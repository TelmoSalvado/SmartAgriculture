<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pt.ipg.smartagriculture">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmartAgriculture">
        <activity android:name=".gftsantos.RegistoActivity"></activity>
        <activity android:name=".gftsantos.LoginActivity" />
        <activity
            android:name=".gftsantos.Slider"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoadingScreen"></activity>
        <activity android:name=".NotMainAnymore"></activity>
    </application>

</manifest>